@charset "UTF-8";
rect {
    fill: none
}
a{cursor: pointer}
image {
    -webkit-user-select: none
}

.product-viewer.active .print-area-border, .product-viewer.active .soft-boundary-border {
    stroke-width: 1;
    stroke: #fff;
    vector-effect: non-scaling-stroke;
    shape-rendering: crispEdges
}

.dnd-image {
    position: absolute;
    pointer-events: none
}

.dnd-image-svg {
    fill: rgba(255, 255, 255, .3);
    margin-left: -40px;
    margin-top: -40px;
    width: 80px;
    height: 80px
}

.configuration-viewer.hide-configuration {
    visibility: hidden
}

.configuration-viewer.hide-configuration * {
    visibility: hidden !important
}

.product-viewer.active.bright .print-area-border, .product-viewer.active.bright .soft-boundary-border {
    stroke: #000
}

.soft-boundary-border {
    stroke-dasharray: 5, 5;
    fill: none
}

.configuration-handle {
    fill: #fff;
    stroke-width: 1;
    stroke: #000;
    vector-effect: non-scaling-stroke;
    visibility: hidden
}

.configuration-handle.active {
    visibility: visible
}

.configuration-handle-icon {
    visibility: hidden
}

.configuration-handle-icon.active {
    visibility: visible
}

.configuration-boundary {
    stroke-dasharray: 9, 5;
    stroke-width: 1;
    stroke: #fff;
    vector-effect: non-scaling-stroke;
    visibility: hidden
}

.configuration-boundary.active {
    visibility: visible
}

.bright .configuration-boundary {
    stroke: #000
}

.text-selection {
    pointer-events: none;
    fill: #64b9e2;
    fill-opacity: .7
}

.configuration-viewer.error .configuration-boundary {
    stroke: red;
    stroke-width: 3
}

.configuration-viewer .rotation-center {
    fill: #ffa53b
}

.configuration-viewer .rotation-circle, .snap-line {
    fill: none;
    stroke-width: 1;
    vector-effect: non-scaling-stroke
}

.text-configuration-background {
    fill: rgba(255, 255, 255, 0);
    fill-opacity: .01
}

.dark .text-cursor {
    stroke: #fff
}

.bright .text-cursor {
    stroke: #00f
}

.text-cursor {
    stroke-width: 1;
    vector-effect: non-scaling-stroke;
    visibility: hidden
}

.text-cursor.active {
    visibility: visible
}

.configuration-viewer .text-selection {
    visibility: hidden
}

.configuration-viewer.active .text-selection {
    visibility: visible
}

