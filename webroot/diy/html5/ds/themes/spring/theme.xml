    <customization>

        <rule id="primaryColor" label="Primary Color" type="color" default="#4c4c4c" group="Colors">
            <css>
                #dsContainer .isd-link-block a:not(.isd-btn), #dsContainer .isd-link-block a:link:not(.isd-btn), #dsContainer
                .isd-custom-selector span,
                #dsContainer .isd-content-nav li a, #dsContainer .isd-content-nav li a:link {
                color:%%value%%;
                }
            </css>
        </rule>
        <rule id="headerColor" label="Header Color" type="color" default="#c4c4c4" group="Colors">
            <css>
                #dsContainer, .isd-header, .isd-header a:not(.isd-btn) {
                color:%%value%%;
                }

            </css>
        </rule>
        <rule id="borderColor" label="Border Color" type="color" default="#c4c4c4" group="Colors">
            <css>
                #dsContainer, .isd-header, #dsContainer .isd-main-content, #dsContainer .isd-content-nav ul, #dsContainer
                .isd-nav-bottom {
                border-top-color:%%value%%;
                border-bottom-color:%%value%%;
                border-color: %%value%%;
                }

            </css>
        </rule>
        <rule id="default_buttonColor" label="Default Button Color" type="color" default="#9a9aa9" group="Colors">
            <css>
                #dsContainer .isd-button-default {
                background-color: %%value%%;
                }

            </css>
        </rule>

        <rule id="default_buttonHover" label="Default Button Hover" type="color" default="#87878e" group="Colors">
            <css>
                #dsContainer .isd-button-default:hover {
                background-color: %%value%%;
                }
            </css>
        </rule>

        <rule id="primary_buttonColor" label="Primary Button Color" type="color" default="#649812" group="Colors">
            <css>
                #dsContainer .isd-button-primary {
                background-color: %%value%%;
                }

            </css>
        </rule>
        <rule id="primary_buttonHover" label="Primary Button Hover" type="color" default="#54810E" group="Colors">
            <css>
                #dsContainer .isd-button-primary:hover {
                background-color: %%value%%;
                }

            </css>
        </rule>

        <rule id="product_slider" label="Product Slider" type="color" default="#4c4c4c" group="Colors">
            <css>
                #dsContainer .isd-product-list-container, #dsContainer .drawer_browseProductsSingle:after{
                background-color: %%value%%;
                }
            </css>
        </rule>

        <rule id="default_title" label="Default Titles" type="color" default="#474747" group="Colors">
            <css>
                #dsContainer .isd-option-span{
                color: %%value%%;
                }
            </css>
        </rule>

        <rule id="default_label" label="Default Headers" type="color" default="#b5b5b5" group="Colors">
            <css>
                #dsContainer .isd-option-header{
                color: %%value%%;
                }
            </css>
        </rule>


        <rule id="header_background" label="Header Background" type="color" default="#474747" group="Colors">
            <css>
                #dsContainer #isd-landing-page-header{
                background-color: %%value%%;
                }
            </css>
        </rule>

        <rule id="choose_product_text" label="Choose Product Header" type="text" default="Choose Product" group="Labels"
              selector="#isd-labelChooseProduct"></rule>
        <rule id="products_text" label="Products Button" type="text" default="Products" group="Labels"
              selector="#isd-labelProducts"></rule>
